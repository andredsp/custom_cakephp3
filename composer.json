{
    "name": "jorge/custom-cakephp3",
    "description": "CakePHP 3 Custom",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "facebook"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Jardim",
            "email": "jorge@jorgejardim.com.br",
            "homepage": "http://www.jorgejardim.com.br/"
        }
    ],
    "require": {
    	"php": ">=5.4.16",
    	"cakephp/cakephp": "~3.0",
    },
    "require-dev": {
	    "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "JCustomCakephp3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JCustomCakephp3\\Test\\": "tests"
        }
    }
}